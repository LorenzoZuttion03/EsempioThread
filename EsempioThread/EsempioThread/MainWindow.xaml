        <Label Content="" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label x:Name="lbl1" Content="" HorizontalAlignment="Left" Height="50" Margin="214,359,0,0" VerticalAlignment="Top" Width="127" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-1.776"/>
